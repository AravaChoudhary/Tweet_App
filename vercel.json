{
    "version": 2,
    "builds": [
      {
        "src": "tweetapp/tweetapp/wsgi.py",
        "use": "@vercel/python",
        "config": {  "maxLambdaSize": "50mb" , "runtime":"python3.9"}
      },
      {
        "src": "biuld_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "tweetapp/tweetapp/wsgi.py"
      }
    ]
  }
 